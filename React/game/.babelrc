{
  "presets": [
    "react-app"
  ],
  "plugins": [
    ["module-resolver", {
      "root": ["./src"],
      "alias": {
        "~components": "./src/app/components",
        "~screen": "./src/app/screen",
        "~redux": "./src/redux",
        "~square": "./src/app/screens/Game/components/Square",
        "~board": "./src/app/screens/Game/components/Board",
        "@login": "./src/app/screens/Login/components/Login"
      }
    }]
  ]
}
